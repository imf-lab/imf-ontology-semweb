@prefix : <http://example.com/imf-aspects#> .
@prefix imf: <http://example.com/imf#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.com/imf-aspects> .

<http://example.com/imf-aspects> rdf:type owl:Ontology ;
                                  owl:versionIRI <http://example.com/imf-aspects/20211130> ;
                                  owl:imports <http://example.com/imf-core/20211130> .

#################################################################
#    Object Properties
#################################################################

###  http://example.com/imf#hasLocation
imf:hasLocation rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf imf:interAspectRelation ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( imf:FunctionAspectObject
                                            imf:ProductAspectObject
                                          )
                            ] ;
                rdfs:range imf:LocationAspectObject .


###  http://example.com/imf#isFulfilledBy
imf:isFulfilledBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf imf:interAspectRelation ;
                  rdfs:domain imf:FunctionAspectObject ;
                  rdfs:range imf:ProductAspectObject ;
                  <http://www.w3.org/2004/02/skos/core#editorialNote> "Erlend: should this be functional and inverse functional?" .


###  http://example.com/imf#isInstalledAs
imf:isInstalledAs rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf imf:interAspectRelation ;
                  rdfs:domain imf:ProductAspectObject ;
                  rdfs:range imf:InstalledAspectObject .


#################################################################
#    Classes
#################################################################

###  http://example.com/imf#FunctionAspectObject
imf:FunctionAspectObject rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Restriction ;
                                               owl:onProperty imf:hasAspect ;
                                               owl:someValuesFrom [ rdf:type owl:Class ;
                                                                    owl:oneOf ( imf:Function
                                                                              )
                                                                  ]
                                             ] ;
                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                           owl:onProperty imf:intraAspectRelation ;
                                           owl:allValuesFrom imf:FunctionAspectObject
                                         ] .


###  http://example.com/imf#InstalledAspectObject
imf:InstalledAspectObject rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty imf:hasAspect ;
                                                owl:someValuesFrom [ rdf:type owl:Class ;
                                                                     owl:oneOf ( imf:Installed
                                                                               )
                                                                   ]
                                              ] ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty imf:intraAspectRelation ;
                                            owl:allValuesFrom imf:InstalledAspectObject
                                          ] .


###  http://example.com/imf#LocationAspectObject
imf:LocationAspectObject rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Restriction ;
                                               owl:onProperty imf:hasAspect ;
                                               owl:someValuesFrom [ rdf:type owl:Class ;
                                                                    owl:oneOf ( imf:Location
                                                                              )
                                                                  ]
                                             ] ;
                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                           owl:onProperty imf:intraAspectRelation ;
                                           owl:allValuesFrom imf:LocationAspectObject
                                         ] .


###  http://example.com/imf#PrimaryAspect
imf:PrimaryAspect rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:oneOf ( imf:Function
                                                    imf:Location
                                                    imf:Product
                                                  )
                                      ] ;
                  rdfs:subClassOf imf:Aspect ;
                  owl:disjointWith imf:SecondaryAspect .


###  http://example.com/imf#ProductAspectObject
imf:ProductAspectObject rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty imf:hasAspect ;
                                              owl:someValuesFrom [ rdf:type owl:Class ;
                                                                   owl:oneOf ( imf:Product
                                                                             )
                                                                 ]
                                            ] ;
                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                          owl:onProperty imf:intraAspectRelation ;
                                          owl:allValuesFrom imf:ProductAspectObject
                                        ] .


###  http://example.com/imf#SecondaryAspect
imf:SecondaryAspect rdf:type owl:Class ;
                    rdfs:subClassOf imf:Aspect .


#################################################################
#    Individuals
#################################################################

###  http://example.com/imf#Function
imf:Function rdf:type owl:NamedIndividual ;
             imf:RDSCodePrefix "=" .


###  http://example.com/imf#Installed
imf:Installed rdf:type owl:NamedIndividual ,
                       imf:SecondaryAspect .


###  http://example.com/imf#Location
imf:Location rdf:type owl:NamedIndividual ;
             imf:RDSCodePrefix "+" .


###  http://example.com/imf#Product
imf:Product rdf:type owl:NamedIndividual ;
            imf:RDSCodePrefix "-" .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( imf:Function
                        imf:Location
                        imf:Product
                      )
] .


