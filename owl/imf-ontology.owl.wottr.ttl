## Relations - specialization


imf:specializationOf a owl:ObjectProperty ;
    skos:prefLabel "specialization of" ;
    vs:term_status "testing" ;
    skos:definition "[specialization of] is a relation that relates a resource *X1* to a an resource *X2* resource to specify that *X1* is a specialization of *X2*. If *X1* is a [specialization of] *X2*, then *X1* inherits all facts expressed for *X2*." ;
    vann:termGroup "logic relations" ;
    # a owl:ReflexiveProperty ;
    a owl:TransitiveProperty ;
    rdfs:subPropertyOf imf:intraAspectRelation, imf:hierarchicalRelation ;
    # rdfs:subPropertyOf skos:narrower ;
    skos:changeNote "0.3.0 :: Added" ;
    .

imf:specializes a owl:ObjectProperty ;
    skos:prefLabel "specializes" ;
    vs:term_status "testing" ;
    skos:definition "[specializes] is the inverse relation of [specialization of].";
    vann:termGroup "logic relations" ;
    # a owl:ReflexiveProperty ;
    a owl:TransitiveProperty ;
    rdfs:subPropertyOf imf:intraAspectRelation, imf:hierarchicalRelation ;
    # rdfs:subPropertyOf skos:broader ;
    owl:inverseOf imf:specializationOf ;
    skos:changeNote "0.3.0 :: Added" ;
    .


## Relations - topology

imf:connectedTo a owl:ObjectProperty ;
    skos:prefLabel "connected to" ;
    vs:term_status "testing" ;
    skos:definition "[connected to] is an [associative relation] and an [intra-aspect relation] that relates an [Element] *E1* to an [Element] *E2* to specify that *E1* is connected to *E2*. [connected to] specifies a topology of [Element]s.";
    # foaf:depiction <http://ns.imfid.org/20230630/img/visualisation-connectedTo.jpg> ;
    vann:termGroup "topology" ;
    a owl:SymmetricProperty  ;
    # a owl:IrreflexiveProperty ; ## outside OWL 2 DL
    rdfs:subPropertyOf imf:intraAspectRelation , imf:associativeRelation ;
    rdfs:domain imf:Element ;
    rdfs:range imf:Element ;
    skos:changeNote "0.1.0 :: Added" ,
        "0.2.0 :: Changed IRI from imf:isConnectedTo to imf:connectedTo." ,
        """0.3.0 :: Changed:
- domain and range from Terminal to Element.
- removed functional axiom.
- added symmetric axiom.
""" ;
    .

imf:adjacentTo a owl:ObjectProperty ;
    skos:prefLabel "adjacent to" ;
    vs:term_status "testing" ;
