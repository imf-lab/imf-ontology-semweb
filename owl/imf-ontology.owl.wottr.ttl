    skos:changeNote "0.3.0 :: Added" ;
    .

imf:colour a owl:AnnotationProperty ;
    skos:prefLabel "colour" ;
    vs:term_status "testing" ;
    skos:definition "[colour] is a relation from a resource *X* to a string identifying a hexadecimal colour *c* to specify that the *c* is a colour that is associated with *X*.";
