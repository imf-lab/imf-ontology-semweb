## Descriptor

imf:Descriptor a owl:Class ;
    rdfs:subClassOf imf:InformationArtefact .

imf:reference a owl:ObjectProperty ;
    skos:prefLabel "reference" ;
    vs:term_status "testing" ;
    skos:definition "[reference] is a generic relation that relates a resource *X* to a resource in an external ontology or reference data library to describe *X*." ;
    skos:scopeNote "This is a generic property that is not intended to be used directly, rather use one of its subproperties. Generic properties like this are introduced to add structure to the ontology's properties, and for expressing generic class constraints." ;
    rdfs:range imf:Descriptor ;
    vann:termGroup "generic relations" ;
    skos:changeNote "0.2.0 :: Added" ;
    .


imf:InformationDomain a owl:Class ;
    skos:prefLabel "Information Domain" ;
    vs:term_status "testing" ;
    # TODO skos:definition "[Information Domain] ..." ;
    rdfs:subClassOf imf:Descriptor ;
    vann:termGroup "aspects" ;
    skos:changeNote "0.3.0 :: Added" ;
    .

imf:Interest a owl:Class ;
    skos:prefLabel "Interest" ;
    vs:term_status "testing" ;
    skos:definition "[Interest] refers to the modelling purpose and the intended usage of the information, and hence the users of the information, their preferences and working habits." ;
    rdfs:subClassOf imf:Descriptor ;
    vann:termGroup "aspects" ;
    skos:changeNote "0.3.0 :: Added" ;
    .

imf:Modality a owl:Class ;
    skos:prefLabel "Modality" ;
    vs:term_status "testing" ;
    skos:definition "[Modality] refers to the mode of existence of what is modelled." ;
    rdfs:subClassOf imf:Descriptor ;
    vann:termGroup "aspects" ;
    skos:changeNote "0.3.0 :: Added" ;
    .

imf:Aspect a owl:Class ;
    skos:prefLabel "Aspect" ;
    vs:term_status "testing" ;
    skos:definition "An [Aspect] sets a context for interpreting and the modelling of [Element]s. An [Aspect] is characterized by an [Information Domain] (What is the modelling focus?), an [Interest] (What is the modelling interest?), and a [Modality] (What is the mode of existence of what is modelled?)." ;
    skos:note "Aspect is a core concept of ISO/IEC 81346 and the IMF ontology includes aspects that are found in ISO/IEC 81346: function (imf:intendedActivity), location (imf:intendedSpace) and product (imf:intendedImplementation)." ;
    rdfs:subClassOf imf:Descriptor ;
    vann:termGroup "aspects" ;
    skos:changeNote "0.1.0 :: Added" ,
        "0.3.0 :: Changed: updated definition to reflect introduced classes [Information Domain], [Interest] and [Modality]." ;
    .

[] ottr:of o-owl-ax:SubObjectExactCardinality ;
    ottr:values ( imf:Aspect "1"^^xsd:nonNegativeInteger imf:hasCharacteristic imf:InformationDomain ) .
[] ottr:of o-owl-ax:SubObjectExactCardinality ;
    ottr:values ( imf:Aspect "1"^^xsd:nonNegativeInteger imf:hasCharacteristic imf:Modality ) .
[] ottr:of o-owl-ax:SubObjectMaxCardinality ;
    ottr:values ( imf:Aspect "1"^^xsd:nonNegativeInteger imf:hasCharacteristic imf:Interest ) .


imf:hasCharacteristic a owl:ObjectProperty ;
    skos:prefLabel "has characteristic" ;
    # TODO skos:definition "[has characteristic] is a relation from an [Aspect] *A* to an entity *X* to specify that *A* is characterized by *X*.";
    vann:termGroup "descriptor" ;
    vs:term_status "testing" ;
    rdfs:subPropertyOf imf:reference ;
    rdfs:domain imf:Descriptor ;
