@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix imf: <http://ns.imfid.org/imf#> .
@prefix pav: <http://purl.org/pav/> .
@prefix o-rdf: <http://tpl.ottr.xyz/rdf/0.1/> .
@prefix o-rdfs: <http://tpl.ottr.xyz/rdfs/0.2/> .
@prefix o-owl-rstr: <http://tpl.ottr.xyz/owl/restriction/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix o-imf-t-o: <http://ns.imfid.org/templates/type/owl/> .
@prefix p14: <http://example.com/P14#> .
@prefix o-imf-t-s: <http://ns.imfid.org/templates/type/shacl/> .
@prefix ex: <http://example.com#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix o-imf: <http://ns.imfid.org/templates/> .
@prefix ottr: <http://ns.ottr.xyz/0.4/> .
@prefix o-owl-ma: <http://tpl.ottr.xyz/owl/macro/0.1/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o-owl-ax: <http://tpl.ottr.xyz/owl/axiom/0.1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix shsh: <http://www.w3.org/ns/shacl-shacl#> .

imf:AspectElementShape
    a sh:NodeShape ;
    sh:property [
        sh:class imf:Aspect ;
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path imf:hasAspect
    ] ;
    sh:targetClass imf:AspectElement ;
    sh:targetSubjectsOf imf:hasAspect .

imf:AttributeShape
    a sh:NodeShape ;
    sh:property [
        sh:class imf:RegularityQualifier ;
        sh:maxCount 1 ;
        sh:path imf:hasAttributeQualifier
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path imf:property
    ], [
        sh:class imf:ScopeQualifier ;
        sh:maxCount 1 ;
        sh:path imf:hasAttributeQualifier
    ], [
        sh:message "Attribute has no value." ;
        sh:minCount 1 ;
        sh:path imf:value ;
        sh:severity sh:Warning
    ], [
        sh:maxCount 1 ;
        sh:path imf:uom
    ], [
        sh:class imf:ProvenenceQualifier ;
        sh:maxCount 1 ;
        sh:path imf:hasAttributeQualifier
    ], [
        sh:message "Attribute has no qualifier." ;
        sh:minCount 1 ;
        sh:path imf:hasAttributeQualifier ;
        sh:severity sh:Warning
    ], [
        sh:class imf:RangeQualifier ;
        sh:maxCount 1 ;
        sh:path imf:hasAttributeQualifier
    ] ;
    sh:targetClass imf:Attribute ;
    sh:targetObjectsOf imf:hasAttribute ;
    sh:targetSubjectsOf imf:hasAttributeQualifier, imf:property, imf:uom .

imf:BlockShape
    a sh:NodeShape ;
    sh:property [
        sh:class imf:Terminal ;
        sh:path imf:hasTerminal
    ], [
        sh:message "The Block has no Terminal." ;
        sh:minCount 1 ;
        sh:path imf:hasTerminal ;
        sh:severity sh:Warning
    ], [
        sh:class imf:Block ;
        sh:maxCount 1 ;
        sh:path imf:partOf
    ] ;
    sh:targetClass imf:Block ;
    sh:targetSubjectsOf imf:hasTerminal .

imf:BreakdownPointShape
    a sh:NodeShape ;
    sh:property [
        sh:class imf:Element ;
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path imf:theWhole
    ], [
        sh:class imf:Element ;
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path imf:thePart
    ] ;
    sh:targetClass imf:BreakdownPoint ;
    sh:targetSubjectsOf imf:thePart, imf:theWhole .

imf:ConnectionPointShape
    a sh:NodeShape ;
    sh:property [
        sh:class imf:Element ;
        sh:maxCount 2 ;
        sh:path imf:theConnected
    ], [
        sh:class imf:Element ;
        sh:maxCount 1 ;
        sh:path imf:theInput
    ], [
        sh:class imf:Element ;
        sh:maxCount 1 ;
        sh:path imf:theOutput
    ] ;
    sh:targetClass imf:ConnectionPoint ;
    sh:targetSubjectsOf imf:theConnected, imf:theInput, imf:theOutput .

imf:ElementShape
    a sh:NodeShape ;
    sh:property [
        sh:message "Element has no prefLabel" ;
        sh:minCount 1 ;
        sh:path skos:prefLabel ;
        sh:severity sh:Warning
    ], [
        sh:message "Element has no classifier" ;
        sh:minCount 1 ;
        sh:path imf:classifier ;
        sh:severity sh:Warning
    ] ;
    sh:targetClass imf:Block, imf:Element, imf:Terminal .

imf:TerminalShape
    a sh:NodeShape ;
    sh:property [
        sh:maxCount 1 ;
        sh:path imf:medium
    ], [
        sh:class imf:Terminal ;
        sh:maxCount 1 ;
        sh:path imf:connectedTo
    ], [
        sh:class imf:Terminal ;
        sh:maxCount 1 ;
        sh:path imf:partOf
    ], [
        sh:in (imf:inputFlow
            imf:outputFlow
        ) ;
        sh:maxCount 1 ;
        sh:path imf:hasTerminalQualifier
    ], [
        sh:message "The Terminal has no connection (to a different Terminal)." ;
        sh:minCount 1 ;
        sh:path imf:connectedTo ;
        sh:severity sh:Warning
    ], [
        sh:message "The Terminal has no medium." ;
        sh:minCount 1 ;
        sh:path imf:medium ;
        sh:severity sh:Warning
    ] ;
    sh:targetClass imf:InputTerminal, imf:OutputTerminal, imf:Terminal ;
    sh:targetObjectsOf imf:connectedTo, imf:hasTerminal ;
    sh:targetSubjectsOf imf:connectedTo, imf:hasTerminalQualifier, imf:medium .

