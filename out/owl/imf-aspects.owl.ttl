@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix imf: <http://ns.imfid.org/imf#> .
@prefix pav: <http://purl.org/pav/> .
@prefix o-rdf: <http://tpl.ottr.xyz/rdf/0.1/> .
@prefix o-owl-rstr: <http://tpl.ottr.xyz/owl/restriction/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix p14: <http://example.com/P14#> .
@prefix ex: <http://example.com#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix o-imf: <http://ns.imfid.org/templates/> .
@prefix ottr: <http://ns.ottr.xyz/0.4/> .
@prefix o-owl-ma: <http://tpl.ottr.xyz/owl/macro/0.1/> .
@prefix o-owl-ax: <http://tpl.ottr.xyz/owl/axiom/0.1/> .
@prefix shsh: <http://www.w3.org/ns/shacl-shacl#> .

imf:Aspect
    a owl:Class .

imf:FunctionAspect
    imf:color "#FFFF00" ;
    imf:prefix "=" ;
    a imf:Aspect .

imf:FunctionAspectElement
    a owl:Class ;
    rdfs:subClassOf imf:AspectElement ;
    owl:equivalentClass [
        a owl:Class, owl:Restriction ;
        owl:hasValue imf:FunctionAspect ;
        owl:onProperty imf:hasAspect
    ] .

imf:FunctionBlock
    rdfs:subClassOf imf:Block, imf:FunctionAspectElement .

imf:FunctionInterfacePoint
    rdfs:subClassOf imf:FunctionAspectElement, imf:InterfacePoint .

imf:FunctionTerminal
    rdfs:subClassOf imf:FunctionAspectElement, imf:Terminal .

imf:InstalledAspect
    imf:color "#FFFFFF" ;
    imf:prefix "::" ;
    a imf:Aspect .

imf:InstalledAspectElement
    a owl:Class ;
    rdfs:subClassOf imf:AspectElement ;
    owl:equivalentClass [
        a owl:Class, owl:Restriction ;
        owl:hasValue imf:InstalledAspect ;
        owl:onProperty imf:hasAspect
    ] .

imf:InstalledBlock
    rdfs:subClassOf imf:Block, imf:InstalledAspectElement .

imf:InstalledInterfacePoint
    rdfs:subClassOf imf:InstalledAspectElement, imf:InterfacePoint .

imf:InstalledTerminal
    rdfs:subClassOf imf:InstalledAspectElement, imf:Terminal .

imf:LocationAspect
    imf:color "#FF00FF" ;
    imf:prefix "+" ;
    a imf:Aspect .

imf:LocationAspectElement
    a owl:Class ;
    rdfs:subClassOf imf:AspectElement ;
    owl:equivalentClass [
        a owl:Class, owl:Restriction ;
        owl:hasValue imf:LocationAspect ;
        owl:onProperty imf:hasAspect
    ] .

imf:LocationBlock
    rdfs:subClassOf imf:Block, imf:LocationAspectElement .

imf:LocationInterfacePoint
    rdfs:subClassOf imf:InterfacePoint, imf:LocationAspectElement, owl:Nothing .

imf:LocationTerminal
    rdfs:subClassOf imf:LocationAspectElement, imf:Terminal, owl:Nothing .

imf:ProductAspect
    imf:color "#00FFFF" ;
    imf:prefix "-" ;
    a imf:Aspect .

imf:ProductAspectElement
    a owl:Class ;
    rdfs:subClassOf imf:AspectElement ;
    owl:equivalentClass [
        a owl:Class, owl:Restriction ;
        owl:hasValue imf:ProductAspect ;
        owl:onProperty imf:hasAspect
    ] .

imf:ProductBlock
    rdfs:subClassOf imf:Block, imf:ProductAspectElement .

imf:ProductInterfacePoint
    rdfs:subClassOf imf:InterfacePoint, imf:ProductAspectElement .

imf:ProductTerminal
    rdfs:subClassOf imf:ProductAspectElement, imf:Terminal .

imf:fulfilledBy
    a owl:ObjectProperty ;
    rdfs:domain imf:AspectElement ;
    rdfs:range imf:AspectElement ;
    rdfs:subPropertyOf imf:InterAspectRelation ;
    owl:inverseOf imf:fulfills .

imf:hasAspect
    a owl:ObjectProperty .

<http://ns.imfid.org/imf-aspects>
    a owl:Ontology ;
    owl:imports <http://ns.imfid.org/20221118/imf-top> ;
    owl:versionIRI <http://ns.imfid.org/20221118/imf-aspects> ;
    skos:altLabel "IMF aspects ontology" ;
    skos:prefLabel "Information Modelling Framework Ontology: Aspects " ;
    skos:scopeNote """

      This ontology defines IMF's central aspects.

    """ .

[]
    a owl:AllDifferent ;
    owl:members (imf:FunctionAspect
        imf:LocationAspect
        imf:ProductAspect
        imf:InstalledAspect
    ) .

