@prefix imf: <http://ns.imfid.org/imf#> .
@prefix pav: <http://purl.org/pav/> .
@prefix o-rdf: <http://tpl.ottr.xyz/rdf/0.1/> .
@prefix o-rdfs: <http://tpl.ottr.xyz/rdfs/0.2/> .
@prefix o-owl-rstr: <http://tpl.ottr.xyz/owl/restriction/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix p14: <http://example.com/P14#> .
@prefix ex: <http://example.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix o-imf: <http://ns.imfid.org/templates/> .
@prefix ottr: <http://ns.ottr.xyz/0.4/> .
@prefix o-owl-ma: <http://tpl.ottr.xyz/owl/macro/0.1/> .
@prefix o-owl-ax: <http://tpl.ottr.xyz/owl/axiom/0.1/> .
@prefix shsh: <http://www.w3.org/ns/shacl-shacl#> .

imf:LocationAspect  imf:color  "#FF00FF" ;
        rdf:type    imf:Aspect ;
        imf:prefix  "+" .

imf:FunctionBlock  rdfs:subClassOf  imf:Block ;
        rdfs:subClassOf  imf:FunctionElement .

imf:FunctionTerminal  rdfs:subClassOf  imf:Terminal ;
        rdfs:subClassOf  imf:FunctionElement .

imf:asFunction  rdfs:range  imf:FunctionElement ;
        rdfs:subPropertyOf  imf:InterAspectRelation ;
        rdf:type            owl:ObjectProperty .

_:b0    rdf:type  owl:Class .

imf:LocationElement  rdf:type  owl:Class ;
        owl:equivalentClass  _:b0 .

_:b0    rdf:type        owl:Restriction ;
        owl:onProperty  imf:hasAspect ;
        owl:hasValue    imf:LocationAspect .

imf:hasAspect  rdf:type  owl:ObjectProperty .

imf:LocationElement  rdfs:subClassOf  imf:AspectElement .

_:b1    rdf:type           owl:Restriction ;
        owl:onProperty     imf:intraAspectRelation ;
        owl:allValuesFrom  imf:LocationElement .

imf:intraAspectRelation
        rdf:type  owl:ObjectProperty .

imf:LocationElement  rdf:type  owl:Class ;
        rdfs:subClassOf  _:b1 .

imf:ProductBlock  rdfs:subClassOf  imf:Block ;
        rdfs:subClassOf  imf:ProductElement .

imf:InstalledAspect  imf:color  "#0000FF" ;
        rdf:type    imf:Aspect ;
        imf:prefix  "::" .

imf:ProductTerminal  rdfs:subClassOf  imf:Terminal ;
        rdfs:subClassOf  imf:ProductElement .

_:b2    rdf:type  owl:Class .

imf:InstalledElement  rdf:type  owl:Class ;
        owl:equivalentClass  _:b2 .

_:b2    rdf:type        owl:Restriction ;
        owl:onProperty  imf:hasAspect ;
        owl:hasValue    imf:InstalledAspect .

imf:hasAspect  rdf:type  owl:ObjectProperty .

imf:InstalledElement  rdfs:subClassOf  imf:AspectElement .

_:b3    rdf:type           owl:Restriction ;
        owl:onProperty     imf:intraAspectRelation ;
        owl:allValuesFrom  imf:InstalledElement .

imf:intraAspectRelation
        rdf:type  owl:ObjectProperty .

imf:InstalledElement  rdf:type  owl:Class ;
        rdfs:subClassOf  _:b3 .

imf:LocationBlock  rdfs:subClassOf  imf:Block ;
        rdfs:subClassOf  imf:LocationElement .

imf:FunctionAspect  imf:color  "#FFFF00" ;
        rdf:type    imf:Aspect ;
        imf:prefix  "=" .

imf:ProductAspect  imf:color  "#00FFFF" ;
        rdf:type    imf:Aspect ;
        imf:prefix  "-" .

imf:asInstalled  rdfs:range  imf:InstalledElement ;
        rdfs:subPropertyOf  imf:InterAspectRelation ;
        rdf:type            owl:ObjectProperty .

imf:asProduct  rdfs:range   imf:ProductElement ;
        rdfs:subPropertyOf  imf:InterAspectRelation ;
        rdf:type            owl:ObjectProperty .

_:b4    rdf:type  owl:Class .

imf:FunctionElement  rdf:type  owl:Class ;
        owl:equivalentClass  _:b4 .

_:b4    rdf:type        owl:Restriction ;
        owl:onProperty  imf:hasAspect ;
        owl:hasValue    imf:FunctionAspect .

imf:hasAspect  rdf:type  owl:ObjectProperty .

imf:FunctionElement  rdfs:subClassOf  imf:AspectElement .

_:b5    rdf:type           owl:Restriction ;
        owl:onProperty     imf:intraAspectRelation ;
        owl:allValuesFrom  imf:FunctionElement .

imf:intraAspectRelation
        rdf:type  owl:ObjectProperty .

imf:FunctionElement  rdf:type  owl:Class ;
        rdfs:subClassOf  _:b5 .

imf:LocationTerminal  rdfs:subClassOf  imf:Terminal ;
        rdfs:subClassOf  imf:LocationElement .

_:b6    rdf:type     owl:AllDifferent ;
        owl:members  _:b7 .

_:b8    rdf:rest   rdf:nil ;
        rdf:first  imf:InstalledAspect .

_:b9    rdf:rest   _:b10 ;
        rdf:first  imf:LocationAspect .

_:b7    rdf:rest   _:b9 ;
        rdf:first  imf:FunctionAspect .

_:b10   rdf:rest   _:b8 ;
        rdf:first  imf:ProductAspect .

imf:InstalledBlock  rdfs:subClassOf  imf:Block ;
        rdfs:subClassOf  imf:InstalledElement .

imf:asLocation  rdfs:range  imf:LocationElement ;
        rdfs:subPropertyOf  imf:InterAspectRelation ;
        rdf:type            owl:ObjectProperty .

imf:InstalledTerminal
        rdfs:subClassOf  imf:Terminal ;
        rdfs:subClassOf  imf:InstalledElement .

_:b11   rdf:type           owl:Restriction ;
        owl:onProperty     imf:intraAspectRelation ;
        owl:allValuesFrom  imf:AspectElement .

imf:intraAspectRelation
        rdf:type  owl:ObjectProperty .

imf:AspectElement  rdf:type  owl:Class ;
        rdfs:subClassOf  _:b11 .

_:b12   rdf:type  owl:Class .

imf:ProductElement  rdf:type  owl:Class ;
        owl:equivalentClass  _:b12 .

_:b12   rdf:type        owl:Restriction ;
        owl:onProperty  imf:hasAspect ;
        owl:hasValue    imf:ProductAspect .

imf:hasAspect  rdf:type  owl:ObjectProperty .

imf:ProductElement  rdfs:subClassOf  imf:AspectElement .

_:b13   rdf:type           owl:Restriction ;
        owl:onProperty     imf:intraAspectRelation ;
        owl:allValuesFrom  imf:ProductElement .

imf:intraAspectRelation
        rdf:type  owl:ObjectProperty .

imf:ProductElement  rdf:type  owl:Class ;
        rdfs:subClassOf  _:b13 .

imf:interAspectRelation
        rdf:type            owl:ObjectProperty ;
        skos:prefLabel      "inter-aspect relation" ;
        rdfs:domain         imf:AspectElement ;
        rdfs:range          imf:AspectElement ;
        rdfs:subPropertyOf  skos:related .

imf:Aspect  rdf:type  owl:Class .

<http://ns.imfid.org/imf-aspects>
        rdf:type        owl:Ontology ;
        owl:versionIRI  <http://ns.imfid.org/20230331/imf-aspects> ;
        skos:prefLabel  "Information Modelling Framework Ontology: Aspects " ;
        skos:altLabel   "IMF aspects ontology" ;
        skos:scopeNote  "\n\n      This ontology defines IMF's central aspects.\n\n    " .

imf:intraAspectRelation
        rdf:type            owl:ObjectProperty ;
        skos:prefLabel      "intra-aspect relation" ;
        rdfs:subPropertyOf  skos:semanticRelation ;
        skos:definition     "A generic relation between aspect objects of the same aspect" .

imf:hasAspect  rdf:type  owl:ObjectProperty ;
        skos:prefLabel   "has aspect" ;
        skos:definition  "Relates an Aspect Element to its Aspect." ;
        rdfs:domain      imf:AspectElement ;
        rdfs:range       imf:Aspect .

imf:AspectElement  rdf:type  owl:Class ;
        rdfs:subclassOf  imf:Element ;
        skos:prefLabel   "Aspect Element" .
