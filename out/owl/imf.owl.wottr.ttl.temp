imf:interAspectRelation
        rdf:type                  owl:ObjectProperty ;
        skos:prefLabel            "inter-aspect relation" ;
        skos:definition           "[inter-aspect relation] a generic relation between [Aspect Element]s *E1* and *E2* such that *E1* and *E2* have different [Aspect]s." ;
        vs:term_status            "testing" ;
        skos:changeNote           "0.1.0 :: Added" ;
        rdfs:subPropertyOf        skos:related ;
        rdfs:domain               imf:AspectElement ;
        rdfs:range                imf:AspectElement ;
        owl:propertyDisjointWith  imf:intraAspectRelation .

imf:hasAttribute  rdf:type  owl:ObjectProperty ;
        skos:prefLabel   "has attribute" ;
        vs:term_status   "testing" ;
        skos:definition  "[has attribute] is a relation between a resource *X* and an [Attribute] *A* that specifies that *X* has the attribute *A*." ;
        skos:changeNote  "0.2.0 :: Added" ;
        rdfs:range       imf:Attribute .

