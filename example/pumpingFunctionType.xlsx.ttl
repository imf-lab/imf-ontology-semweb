ex:sB-001
    a imf:BlockType, sh:NodeShape ;
    sh:name "Pumping, liquid velocity" ;
    sh:property [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sT-001 ;
        sh:path imf:hasTerminal
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sT-002 ;
        sh:path imf:hasTerminal
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAg-002 ;
        sh:path imf:hasAttributeGroup
    ], [
        sh:hasValue imf:functionAspect ;
        sh:path imf:aspect
    ], [
        sh:hasValue "SP402" ;
        sh:path imf:symbol
    ], [
        sh:hasValue <http://example.net/rdl/Pumping> ;
        sh:path imf:classifier
    ], [
        sh:maxCount 3 ;
        sh:minCount 1 ;
        sh:node ex:sT-003 ;
        sh:path imf:hasTerminal
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAg-001 ;
        sh:path imf:hasAttributeGroup
    ] ;
    sh:targetClass ex:sB-001 .

ex:sBAg-001
    a imf:AttributeGroupType, sh:NodeShape ;
    sh:name "Pumping purpose attributes" ;
    sh:property [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-001 ;
        sh:path imf:hasAttribute
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-002 ;
        sh:path imf:hasAttribute
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-003 ;
        sh:path imf:hasAttribute
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-004 ;
        sh:path imf:hasAttribute
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-005 ;
        sh:path imf:hasAttribute
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-006 ;
        sh:path imf:hasAttribute
    ] ;
    sh:targetClass ex:sBAg-001 .

ex:sBAg-002
    a imf:AttributeGroupType, sh:NodeShape ;
    sh:name "Pumping supplementary attributes" ;
    sh:property [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-007 ;
        sh:path imf:hasAttribute
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-008 ;
        sh:path imf:hasAttribute
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node ex:sBAttr-009 ;
        sh:path imf:hasAttribute
    ] ;
    sh:targetClass ex:sBAg-002 .

ex:sBAttr-001
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Fluid" ;
    sh:property [
        sh:datatype xsd:integer ;
        sh:maxCount 1 ;
        sh:maxInclusive 10000 ;
        sh:minCount 1 ;
        sh:minInclusive 0 ;
        sh:path imf:value
    ] ;
    sh:targetClass ex:sBAttr-001 .

ex:sBAttr-002
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Flowrate" ;
    sh:targetClass ex:sBAttr-002 .

ex:sBAttr-003
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Pressure delta" ;
    sh:targetClass ex:sBAttr-003 .

ex:sBAttr-004
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Suction pressure" ;
    sh:targetClass ex:sBAttr-004 .

ex:sBAttr-005
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Pressure delta" ;
    sh:targetClass ex:sBAttr-005 .

ex:sBAttr-006
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Pumping power" ;
    sh:targetClass ex:sBAttr-006 .

ex:sBAttr-007
    a imf:AttributeType, sh:NodeShape ;
    sh:name "App standard" ;
    sh:targetClass ex:sBAttr-007 .

ex:sBAttr-008
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Client reqs" ;
    sh:targetClass ex:sBAttr-008 .

ex:sBAttr-009
    a imf:AttributeType, sh:NodeShape ;
    sh:name "Noise regs" ;
    sh:targetClass ex:sBAttr-009 .

ex:sT-001
    a imf:TerminalType, sh:NodeShape ;
    sh:property [
        sh:hasValue <http://example.net/rdl/RDS4233125> ;
        sh:path imf:classifier
    ], [
        sh:hasValue imf:input ;
        sh:path imf:direction
    ], [
        sh:hasValue <http://example.net/rdl/WaterMedium> ;
        sh:path imf:media
    ], [
        sh:hasValue <http://example.net/rdl/RDS4233127> ;
        sh:path imf:classifier
    ], [
        sh:hasValue imf:output ;
        sh:path imf:direction
    ], [
        sh:hasValue <http://example.net/rdl/WaterMedium> ;
        sh:path imf:media
    ] ;
    sh:targetClass ex:sT-001 .

ex:sT-002
    a imf:TerminalType, sh:NodeShape ;
    sh:property [
        sh:hasValue imf:input ;
        sh:path imf:direction
    ], [
        sh:hasValue <http://example.net/rdl/MechanicalRotatation> ;
        sh:path imf:media
    ] ;
    sh:targetClass ex:sT-002 .

ex:sT-003
    a imf:TerminalType, sh:NodeShape ;
    sh:targetClass ex:sT-003 .

